 *
 */

public class Ex09 {
	
	public static void main(String[] args) {

		// char [] -> String 변환
		// for문
		// String.chatAt()
		
		String todo = "Auto-generated method stub";
		//System.out.println(todo.length());
		int size = todo.length();
		
		char [] todoCharArr = new char[size];
		
		// todoCharArr.length
		
		/*
		 * 
		 * todoCharArr[0] = todo.charAt(0)
		 * todoCharArr[i] = todo.charAt(i)
		 * todoCharArr[i] = todo.charAt(i)
		 * todoCharArr[i] = todo.charAt(i)
		 * todoCharArr[4] = todo.charAt(4)
		 * 
		 */
		
		for (int i = 0; i < todoCharArr.length; i++) {
			todoCharArr[i] = todo.charAt(i);
		}		
		
		// char [] -> String 변환
		
		
		//  String -> char[] 변환
		
		String target = "";
		for (int i = 0; i < todoCharArr.length; i++) {
			target += todoCharArr[i];
			
		}
		System.out.println(target);
		
		
		
		
		
	}
	
}
