package days13;

/**
 * @author jyoon
 * @date 2024. 7. 17. - 오전 11:50:18
 * @content 	private으로 선언된 필드를 외부에서 접근하게 하는 방법 ?
 * @subject 	Person.java                   
 *                        
 *
 */

public class Ex04 {
	
	public static void main(String[] args) {
		// Person 클래스 선언
		Person p1 =new Person();
		// p1 객체의 필드를 초기화
		//		ㄴ 1) 생성자
		// 		ㄴ 2) getter, setter
		
		p1.setAge(-20);
		System.out.println(p1.getAge());
		
		

	} // main

	
	
} // class
