package days06;

public class Type {
	public static void main(String[] args) {
		
		
		int sum =0;
		boolean sw = false;
		
		for (int i = 1; i <= 10; i++) {
			if (!sw) {
				System.out.printf("%d-", i);
			} else {
				System.out.printf("%d+",i);
				sum-=i;
			}
			
			sw = !sw;
		}
		
		System.out.println(sum);
	
	}
	
}
