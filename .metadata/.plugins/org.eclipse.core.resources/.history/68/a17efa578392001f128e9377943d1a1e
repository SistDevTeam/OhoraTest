package days12;

/**
 * @author jyoon
 * @date 2024. 7. 16. - 오후 3:44:20
 * @content 객체(클래스) 배열
 * @subject                       
 *                 
 *                        	국어점수 5000명	
 *                      	int kor = 90;
 *                        	int kor0001 = 100;
 *                        				:
 *							int kor5000 = 93;				
 *							배열
 *							int [] kors = new in [500000];
 *
 *							Point p0001 = new Point();	
 *										:
 *							Point p5000 = new Point();
 *
 */

public class Ex08 {

	public static void main(String[] args) {
		// points는 객체명이 아니라 배열명이다.
		
		Point [] points = new Point[5000]; // 배열 선언
		
		points[0] = new Point(); // 인스턴스화 시키는 작업이 빠졌었다~
		
		points[0].x =0; // java.lang.NullPointerException 객체 생성이 안된 상황
		points[0].y =0;
		points[0].dispPoint();

		

	} // main

} // class
