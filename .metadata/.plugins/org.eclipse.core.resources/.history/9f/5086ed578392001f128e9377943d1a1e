package days04;

public class Ex10 {

	public static void main(String[] args) {


		/* 1~10까지의 합 for 문

		int sum =0;

		for(int i = 1; i<=10; i++) {
			System.out.printf("%d+",i);
			sum += i;
		}
		System.out.printf("=%d\n",sum);
		 */

		// 3번 방법 
		int i = 0, sum =0;

		while(++i<=10) { // 5<=10 참
			if(i==5) continue;
			
			System.out.printf("%d+",i); 
			sum +=i;
			
		} // while

		System.out.printf("=%d\n",sum);

		
	
	} // main

} // class
