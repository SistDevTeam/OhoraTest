package days15;

public interface Engine {
    
	// method 
	// 몸체가 있는 인스턴스 메서드 이기 때문에 안됨, 그래서 추상 메서드로 만듦
	public abstract void moreFuel(int fuel);
	void lessFuel(int fuel);
	void stop();
	
	static void staticMethod() {
		//
	}
	
	
	default void newMethod() { 
		// 몸체 구현...
	}
	
	// void newMethod();
	
}
