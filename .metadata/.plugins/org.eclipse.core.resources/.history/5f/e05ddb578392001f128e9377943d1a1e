public class Ex03_03 {
	
	
	public static void main(String[] args) {
		// Object...args
		// PrintStream 리턴자료형
		System.out.printf("출력형식")
		.printf("홍길동")
		.printf("XXX")
		.printf("OOO")
		;
	
	}
	

}
