package days15;

import java.util.ArrayList;
import java.util.List;
import java.util.Vector;

/**
 * @author jyoon
 * @date 2024. 7. 19. - 오후 12:19:25W
 * @content 
 * @subject                       
 *                        
 *
 */

public class Ex04 {
	
	public static void main(String[] args) {
		
		// 다형성
		List list = new ArrayList(); // 업캐스팅 상속관계 (다형성의 확장성)
		
		// 매개변수 다형성
		list.add(new ArrayList());
		list.add(new Vector());
		list.add("홍길동"); // 매개변수 타입이 Object 이기 때문에 뭐가 와도 상관 없음
		list.add(true); // 매개변수 타입이 Object 이기 때문에 뭐가 와도 상관 없음
		
		
		
	} // main

} // class
