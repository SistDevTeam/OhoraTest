package days13;

import java.util.Scanner;

public class Ex09 {
	
	static String name;
	
	public static void main(String[] args) {
		
		Math.max(0, 0);
		Math.abs(0);
		Math.random();
		Math.pow(0,0); // Math 메서드는 스태틱임
		
		// System.out
		// System.in
		
		Scanner scanner = new Scanner(System.in); // 괄호안이 생성자이다. 필드를 초기화함
		
		
		
		
		
		// 클래스명.필드
		// 클래스명.메서드()
		name = "홍길동";
		
		// Ex09 obj = new Ex09();
		// int result = obj.sum(1,2);
		
		
		
	}
	
	
	public static void test() {
		Ex09.name = "김길동";
	}
	
	

}
