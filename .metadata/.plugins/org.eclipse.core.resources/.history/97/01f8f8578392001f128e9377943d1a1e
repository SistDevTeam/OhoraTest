package days11;

public class Ex04 { 

	// java.exe 로 java Ex04 -> 시작개체 + main() -> Ex04.main() ->  
	public static void main(String[] args) { // 프로그램 시작 String[] args 이놈은 대체 무엇? 

		for (int i = 0; i < args.length; i++) {
			System.out.println( args[i] );
		}

		System.out.println(" end ");




	} // main 프로그램 종료

} // class
