package days02;

/**
 * @author jyoon
 * @date 2024. 7. 2. - 오후 12:25:06
 * @subject
 * @content
 *
 *
 */

public class Ex03 {

	public static void main(String[] args) {
		
		// println() 메서드 - 출력
		System.out.println("전재윤");
		System.out.println("이창익");
		System.out.println("송세호");
		
		// print() 메서드 - 출력
		System.out.print("전재윤\n");
		System.out.print("전재윤\r");
		System.out.print("전재윤" + '\n');
		System.out.print("이창익" + '\r');
		System.out.print("송세호");
		System.out.println(); // 개행을 목적으로만 추가도 가능
		System.out.print("이시훈");
			
		// 개행 (줄바꿈 여부) == CR '\r' + LF '\n' 커서가 제일 앞으로 땡겨지고 + 다음 라인으로 이동
		
	}
	
}
