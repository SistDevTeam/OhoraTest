package days16;

public class Ex05_03 {

	public static void main(String[] args) {
		String [] urlArr = {
				"www.naver.com"
				, "http://www.naver.com"
				, "http://www.sist.co.kr"
				, "https://www.daum.net"
		};

		// String regex = "^http://";  이건 접수어가 http로 시작해야 한다는 것이고 [^0-9] 이건  0-9가 오면 안된다는 것으로 쓰인다. 둘의 사용처가 반대이다.
		// String regex = ".com$"; 끝나는 문자열
		
		for (int i = 0; i < urlArr.length; i++) {
			//if( urlArr[i].startsWith("http://") )
			if( urlArr[i].endsWith(".com") )
			   System.out.println(urlArr[i]);
		}

	}

}
