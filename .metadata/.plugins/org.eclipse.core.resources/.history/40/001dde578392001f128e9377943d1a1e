package days03;

/**
 * @author jyoon
 * @date 2024. 7. 3. - 오후 3:44:38
 * @subject [비교연산자]
 * @content 크다, 작다, 같거나 크다, 같거나 작다, 같다, 다르다
 *				   비교연산의 결과는 boolean이다. (true, false)
 *

 */

public class Ex05_03 {

	public static void main(String[] args) {
		
		int i =10, j =3;
		System.out.println(i>j); // true
		System.out.println(i>=j); // true
		System.out.println(i<j); // false
		System.out.println(i<=j); // false
		
		// (주의할 점)
		System.out.println(i=j); // 대입 연산자라 3나옴 j를 i에 넣음
		System.out.println(i==j); // false
		System.out.println(i!=j); // true 부정하고 대입
		
		
		
	}

}
