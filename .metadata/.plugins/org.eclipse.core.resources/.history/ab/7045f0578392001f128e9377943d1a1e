public class Ex03 {
	
	public static void main(String[] args) {
		
		char one;
		
		Scanner scanner = new Scanner(System.in);
		System.out.print("한 문자를 입력하시오.");
	
		one = scanner.next().charAt(0);
		
		System.out.println(one);
		
		
		
		
		
	} // main

} // class


