package days05;

import java.util.Scanner;

public class Ex01 {

   public static void main(String[] args) {

      Scanner scanner = new Scanner(System.in);
      System.out.print("문자를 입력하세요 ? ");
      String data = scanner.nextLine();
     
         System.out.printf("출력 : %s\n",data);

   } // main

} // class






