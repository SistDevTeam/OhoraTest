package days07;

import java.util.Scanner;

public class Ex02_03_02 {
	
	public static void main(String[] args) {
//      이등변삼각형
//      __*             (1,3)
//      _*** 
//      *****
		
           
for (int i = 1; i <=3; i++) {
	for (int j = 1; j <=5; j++) {
		//System.out.printf(i+j>=4?"*":"_");
		//System.out.printf(j-i<=2?"*":"_");
		System.out.printf(i+j>=4 && j-i<=2?"*":"_");
	}
	
	System.out.println();
	
}

		
		
	} // main

} // class
